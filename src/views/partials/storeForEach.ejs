<% stores.forEach(store => { %>
    <article class="main__store__section__article">
        <img src="/img/<%= store.image || 'default-image.png' %>" alt="">
        <button class="button--heart"><i class="fa-solid fa-heart"></i></button>
        <div class="main__store__section__article__div">
            <a href="/storesCadets/detail/<%= store.id %>">entrar a la tienda</a>
            <a href="">contacto</a>
        </div>
    </article>  
<% }) %>